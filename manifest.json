{
    "name": "USCRAPPER",
    "description": "Extension with AI Integration",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "src/html/popup.html",
      "default_icon": "src/assets/uscrapper_icon.png"
